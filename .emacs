;;;; Where to find things

(defconst -load- "~/.emacs.d/lisp/")
(defun lpath/ (s) (concat -load- s))

;; (defmacro recursive-add-to-load-path (p)
;;   `(let ((default-directory ,p))
;;      (normal-top-level-add-subdirs-to-load-path)))

(add-to-list 'load-path (lpath/ "config"))

(let ((default-directory (lpath/ "config")))
  (normal-top-level-add-subdirs-to-load-path))

(let ((default-directory (lpath/ "packages")))
  (normal-top-level-add-subdirs-to-load-path))

(setq custom-theme-load-path
      (append custom-theme-load-path
              (list (lpath/ "packages/emacs-color-theme-solarized")
                    (lpath/ "packages/color-theme-base16")
                    (lpath/ "packages/color-theme-mnml-light"))))

(add-to-list 'exec-path "/usr/local/bin")


