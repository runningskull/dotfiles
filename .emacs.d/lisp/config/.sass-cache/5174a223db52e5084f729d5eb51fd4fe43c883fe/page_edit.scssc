@mixin very-subtle-button($x:1, $color:$swatch-blue) {
    @include box-shadow(inset 0 -29px 1px rgba($color, (0.02 * $x)));
}

@mixin edit-button {
    z-index:            2;

    position:           absolute;
    height:             100%;
    padding:            0;

    right:              0;

    width:              75px;
                
    i {
        display:            block;
        width:              47px;
        height:             21px;

        position:           absolute;
        top:                50%;
        margin-top:         -10px;
        left:               50%;
        margin-left:        -23px;

        background:         url(/static/img/icons/edit-page-sprite.png);
        background-repeat:  no-repeat;
    }
}

body.edit-raffle {
    .rafl-list {
        li {
            @include border-radius(3px);

            position:       relative;
            overflow:       visible;

            border:         1px solid $swatch-faintgrey;
            margin-bottom:  em(10);

            white-space:    nowrap;
            padding-right:  90px;

            &:hover, &.editing {
                background: $swatch-blue;
                border:     1px solid $swatch-blue;

                cursor:     pointer;

                .edit-button {
                    &, &:hover, &:active {
                        i {
                            @include sprite-rect(0, 21px);
                        }
                    }
                }
                
                * {
                    color:  #fff !important;
                }

                .delete {
                    color:  $swatch-lgrey !important;

                    &:hover {
                        color:  #fff !important;
                    }
                }
            }

            .mandatory-icon {
                display:        none;
                width:          14px;
                height:         21px;
                position:       absolute;
                top:            50%;
                margin-top:     -11px;
                left:           0;
                margin-left:    10px;
            }

            &.mandatory, &.mandatory ~ li {
                $_padding:  4.5%;
                
                padding-left:   $_padding;

                .edit-box {
                    width:  100% - $_padding;
                }

                .mandatory-icon {
                    background-image:   url(/static/img/icons/edit-page-sprite.png);
    
                    display:    block;
                    position:   absolute;
                    left:       0;
                }
            }

            &.mandatory {
                border-color:   $swatch-lgrey;

                .mandatory-icon {
                    @include sprite-rect(47px, 0);
                }

                &:hover {
                    border-color:   $swatch-blue;
                }
            }

            &.mandatory ~ li {
                .mandatory-icon {
                    @include sprite-rect(47px, 21px);
                }
            }

            &.deleted {
                border-color:   mix($swatch-faintgrey, #fff, 48%);
                border-style:   dashed !important;

                &:hover {
                    background-color:   $swatch-orange;
                }

                a {
                    @include transition(0.1s);
                    padding:    0;

                    * {
                        color:              mix($swatch-faintgrey, #fff, 25%) !important;
                        font-size:          0.75em;
                        text-decoration:    line-through;
                    }
                }

                .edit-button {
                    display:    none;
                }

                .delete {
                    display:    none;
                }

                .undo-button {
                    display:    block;
                    color:      $swatch-lgrey !important;

                    &:hover {
                        background-color:   $swatch-blue;
                        color:              #fff !important;
                    }
                }
            }

            //.nocssanimations & {
                &.ping, &.unping {
                    border-color: $swatch-green;
                }

                &.ping {
                    @include transform(scale(1.025));
                    @include transition(0.1s);

                    $_w: 0;
                    $_c: $swatch-green;
                    $_b: 1px;
                    @include box-shadow($_w     0       $_b     $_c,
                                        -$_w    0       $_b     $_c,
                                        0      (-$_w)   $_b     $_c,
                                        0       $_w     $_b     $_c);
                }

                &.unping {
                    @include transform(scale(1));
                    @include box-shadow(none);
                    @include transition(0.1s);
                }
            //}

            //&.cssanimations & {
            //    &.ping {
            //        // TODO: for some reason this isnt compiling.
            //        //       i think it has to do with libsass but not sure
            //        /*@include keyframes(tada) {*/
            //            /*0% {                @include transform(scale(1));}   */
            //            /*10%, 20% {          @include transform(scale(0.9) rotate(-3deg));}*/
            //            /*30%, 50%, 70%, 90% {@include transform(scale(1.1) rotate(3deg));}*/
            //            /*40%, 60%, 80% {     @include transform(scale(1.1) rotate(-3deg));}*/
            //            /*100% {              @include transform(scale(1) rotate(0));}*/
            //        /*}*/

            //        /*-webkit-animation-name: tada;*/
            //        /*-moz-animation-name: tada;*/
            //        /*-o-animation-name: tada;*/
            //        /*animation-name: tada;*/
            //    }
            //}

            a {
                @extend .large-input;
                text-decoration:    none;
                overflow:           hidden;

                &:hover {
                    text-decoration:    underline;
                }
            }

            .meta {
                @include weight-semibold;

                font-family:    $font-stack-mono;
                font-size:      em(14);

                /*position:       relative;*/
                margin-top:     -1px;

                margin-right:   1px;

                color:          $swatch-faintgrey;
                letter-spacing: 1px;

                em {
                    @include translateY(-10px);
                    padding-left:   1px;
                    padding-right:  1px;
                    font-size:      em(12, 14);
                    color:          $swatch-lgrey;

                    &:hover {
                        text-decoration:    none;
                    }
                }
            }

            .delete, .undo-button {
                $_size: 20px;

                @include weight-light;
                @include border-left-radius(3px);   // not working w/ libsass

                position:       absolute;
                right:          100%;
                margin-right:   1px;
                width:          $_size;
                height:         $_size;

                top:            50%;
                margin-top:     -($_size / 2);

                text-align:     center;
                line-height:    $_size + 2px;

                padding:        0;
                background:     mix($swatch-faintgrey, #fff, 20%);
                color:          $swatch-lgrey;

                &:hover {
                    @include transition(0.15s);
                    @include opacity(1);

                    text-decoration:    none;
                    background:         $swatch-orange;
                    margin-right:       2px;
                }
            }

            .undo-button {
                display:    none;
            }
        }

        .edit-box {
            position:   absolute;
            width:      100%;
            height:     100%;

            a span {
                display:    none;
            }

            .edit-button {
                @include edit-button;
            }
        }
    }

    #main-col {
        h2 {
            position:   relative;
        }
    }

    .poll_opt {
        &.introducing {
            @include transition(0.25s);
            margin-top: 10px;
        }
    }

    #dates {
        fieldset {
            float:  left;
            width:  45%;

            &.end {
                float:  right;

                h2 {
                    margin-left:    -em(17);
                }
            }

            em {
                font-style:     normal;
                color:          $swatch-faintgrey;
                margin-right:   em(2);
                margin-left:    em(2);
            }

            i {
                @include inline-block;
                height:         24px;
                width:          24px;

                margin-right:   em(8);

                background:     url(/static/img/icons/edit-page-sprite.png);

                &.date { @include sprite-rect(0, 42px); }
                &.time { @include sprite-rect(37px, 42px); }
            }
        }

        .line {
            display:        block;
            overflow:       hidden;
            
            margin-bottom:  1em;

            i {
                @include inline-block;
                width:  24px;


            }
        }

        small {
            font-family:    Helvetica, sans-serif;
            font-size:      em(10);
            font-style:     italic;
            letter-spacing: 1px;

            background:     mix($swatch-faintgrey, #fff, 20%);
            color:          $swatch-lgrey;
            padding:        0.5em 1em;
            display:        block;
            line-height:    1.5em;

            a {
                margin:         0 2px;
                text-transform: uppercase;
                letter-spacing: 0;
                font-style:     normal;
                color:          $swatch-green;
            }
        }

        input[type="text"] {
            display:    inline;
            width:      em(120);
            padding:    em(8);
            border:     none;
            color:      $swatch-lgrey;
        }

        #dates-split {
            width:  40px;
            float:  left;
            height: 100px;
        }
    }
    
    .button-add {
        @include weight-normal;
        @include border-radius(3px);

        font-size:          em(14);

        display:            block;
        border:             1px dashed mix($swatch-faintgrey, #fff, 40%);

        line-height:        em(16, 14);
        padding:            22px 20px 18px 20px;

        position:           relative;

        color:              mix($swatch-lgrey, #fff, 50%);
        letter-spacing:     1px;

        border-width:       2px;

        text-decoration:    none;
        text-transform:     uppercase;

        &:hover {
            @include text-shadow(1px 1px 0 rgba(#000, 0.04));

            border-style:       solid;

            font-size:          em(14, 14);
            padding-top:        21px;
            padding-bottom:     17px;

            /*color:              mix($swatch-lgrey, #fff, 70%);*/
            color:              #fff;
            /*background:         $swatch-green;*/
            /*background:         $swatch-lgrey;*/
            background:         $swatch-blue;
            /*border-color:       mix($swatch-faintgrey, #fff, 25%);*/
            /*border-color:       $swatch-green;*/
            /*border-color:       $swatch-lgrey;*/
            border-color:       $swatch-blue;

            i {
                @include _button-chevron("blue", "up");
            }

            b {
                text-decoration:    none;
                top:                -1px;
            }
        }

        &:active {
            @include text-shadow(none);
        }

        b {
            @include inline-block;
            @include weight-semibold;

            font-size:      0.875em;
            position:       relative;
            top:            -2px;
            margin-right:   6px;
        }
    }

    #prize-imgselect {
        padding-top:    2em;
        padding-bottom: 1em;

        img {
            margin-bottom:  2em;
        }
    }

    #prize-img-edit {
        display:    none;
    }

    #rm-prize-img {
        @include weight-normal;
        font-family:    $font-stack;
        text-transform: uppercase;
        font-style:     normal;

        a {
            color:  $swatch-lgrey;
        }
    }

    #add-prize-img {
        @include button("lgrey");

        &, &:hover, &:active {
            line-height:    2em;
            height:         2em;
        }

        &.existing, &.existing:hover, &.existing:active {
            float:          right;
        }
    }

    #comments {
        .markItUpHeader {
            @include opacity(0.64);

            &:hover {
                @include transition(all 0.15s);
                @include opacity(1);
            }
        }
    }

    #id_comments {
        font-family:    Georgia, serif;
        font-size:      0.5em;
        font-style:     italic;

        color:          $swatch-faintgrey;

        &:focus {
            @include box-shadow(0 8px 12px rgba(#000, 0.2));
            font-size:      1em;
            font-style:     normal;
            border:         10px solid mix($swatch-faintgrey, #fff, 20%);
            width:          $main-col-width * 1.25;
            position:       absolute;
            right:          0;
            height:         15em;
            z-index:        2000;

            color:          $swatch-lgrey;

            line-height:    1.4em;
        }
    }
    
    #terms-template-init {
        color:          $swatch-blue;
        font-size:      0.75em;
        text-decoration:    none;

        span {
            text-decoration:    underline;
        }

        b {
            font-size:          1.25em;
            text-decoration:    none;
        }
    }

    #terms-edit-form {
        input[type="text"] {
            padding:    0.5em;
        }
    }

    #refer-box {
        @include border-radius(3px);
        padding:        1em 0.675em;
        background:     mix($swatch-faintgrey, #fff, 20%);
        margin-bottom:  1em;

        position:       relative;

        label {
	    @include weight-normal;
	    font-size: 1em;
	    margin: 0;
	    
	    &:hover {
		text-decoration:    underline;
		cursor:             pointer;
            }
	}

        input:checked ~ span {
            color:  $swatch-dgrey;
        }

        input {
            margin-right:   0.675em;
        }
    }

    #question-refer {
        @extend .circle-help-button;
        height:             19px;
        width:              19px;
        line-height:        20px;
        font-size:          em(13);
        margin-right:       13px;
        margin-top:         -1px;
    }

    #choose-score {
        label {
            @include inline-block;
            margin-right:   0.5em;
            position:       relative;
            top:            -0.25em;
        }

        a {
            @include inline-block;

            padding:            0.325em 0.75em;
            line-height:        1.125em;
            height:             1em;

            border:             1px solid $swatch-faintgrey;
            text-decoration:    none;
            text-align:         center;

            margin-right:       -5px;

            color:              $swatch-lgrey;

            &:hover {
                background: mix($swatch-faintgrey, #fff, 20%);

                i {
                    color:  $swatch-lgrey;
                }
            }

            i {
                @include weight-semibold;
                font-size:      0.75em;
                position:       relative;
                top:            -1px;
                margin-right:   3px;
            }

            &.active {
                color:          #fff;
                background:     $swatch-lgrey;
                border-color:   $swatch-lgrey;

                i {
                    color:          #fff;
                }
            }
        }
    }

    section.last {
        overflow:   hidden;
        border-top: 1px solid $swatch-faintgrey;
    }

    .spinner, .success {
        position:       absolute;
        right:          0;
        top:            50%;
        margin-top:     -12px;
        margin-right:   4px;
    }
    
    #rc-disclaimer-full {
        font-family:    Georgia, serif;
        font-style:     italic;
        font-size:      0.875em;
        color:          $swatch-lgrey;

        border:         2px dashed $swatch-faintgrey;
        padding:        1em;
        line-height:    1.4em;
        
        p {
            margin-bottom:  1em;
        }
    }

    #entry-option-dropdown {
        overflow:       hidden;
        margin-top:     20px;

        border:         4px solid mix($swatch-faintgrey, #fff, 20%);

        li {
            display:        table;
            float:          left;
            padding:        0;
            margin:         0;
            width:          50%;

            a {
                border:             4px solid #fff;//mix($swatch-faintgrey, #fff, 20%);
                padding:            0 1.5em 0 72px;

                display:            table-cell;
                vertical-align:     middle;

                text-decoration:    none;
                color:              $swatch-lgrey;

                height:             em(64);

                position:           relative;

                background:         #fff;

                &:hover {
                    $_color: mix($swatch-faintgrey, #fff, 32%);
                    border-color:       $_color;
                    z-index:            3;
                }

                i {
                    background-image:   url(/static/img/icons/option-types.png);
                    width:              $dropdown-icon-size;
                    height:             $dropdown-icon-size;
                    position:           absolute;

                    left:               0;
                    margin-left:        4px;

                    top:                50%;
                    margin-top:         -($dropdown-icon-size / 2);
                }

                span {
                    display:        table-cell;
                }
            }
        }
    }

    #final-step {
        color:      $swatch-lgrey;

        .inner {
            width:      $inner-width - 280px;
            padding:    120px;
        }

        .statement {
            @include weight-normal;
            font-size:      em(48);
            position:       absolute;
            left:           140 120px;

            line-height:    1em;
            margin-top:     1px;
        }
    }

    #submit-link {
        @include button("blue", "right");
        float:  right;

         b {
            font-size:  1.25em;
         }
    }

    #opt-dropdown-facebook  i { @include sprite-rect(0); }
    #opt-dropdown-twitter   i { @include sprite-rect($dropdown-icon-size * 1); }
    #opt-dropdown-twfollow  i { @include sprite-rect($dropdown-icon-size * 1); }
    #opt-dropdown-pinit     i { @include sprite-rect($dropdown-icon-size * 2); }
    #opt-dropdown-pinfollow i { @include sprite-rect($dropdown-icon-size * 2); }
    #opt-dropdown-poll      i { @include sprite-rect($dropdown-icon-size * 3); }
    #opt-dropdown-comment   i { @include sprite-rect($dropdown-icon-size * 4); }
    #opt-dropdown-emlist    i { @include sprite-rect($dropdown-icon-size * 5); }
    #opt-dropdown-custom    i { @include sprite-rect($dropdown-icon-size * 6); }
}

