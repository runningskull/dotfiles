@import "newhome/_buttons";
@import "compass";
@import "compass/reset";

@import "global";
@import "_triangle";



@mixin tooltip {
    $_arrw: 10px;

    @include weight-light;
    @include text-shadow(1px 1px 0 rgba($swatch-dgrey, 0.5));
    @include box-shadow(inset 0 1px 0 rgba(#fff, 0.1),
                        0 8px 12px rgba(#000, 0.12));
    position:       absolute;
    color:          #fff;
    background:     $swatch-lgrey;
    padding:        0.75em 1em 0.75em 0.75em;
    line-height:    1.3em;
    font-size:      0.875em;
    z-index:        999999;
    border:         1px solid $swatch-dgrey;

    max-width:      400px;

    .tooltip-arrow {
        position:       absolute;
        width:          0;
        height:         0;
        border-color:   transparent;
        border-style:   solid;
    }

    &.left {
        margin-left:    -$_arrw - 8px;

        .tooltip-arrow {
            top:                50%;
            left:               100%;
            margin-top:         -$_arrw;
            border-width:       $_arrw 0 $_arrw $_arrw;
            border-left-color:  $swatch-lgrey;
        }
    }
}

.tooltip { @include tooltip; }


.circle-help-button {
    @include inline-block;
    @include border-radius(100px);
    @include weight-semibold;

    z-index:            99;

    position:           absolute;
    right:              0;

    color:              #fff;
    background:         $swatch-lgrey;
    text-decoration:    none;
    text-align:         center;

    i {
        margin-left:    -1px;
        text-style:     normal;
        position:       relative;
    }

    #sidebar & {
        width:      14px;
        height:     14px;
        font-size:  0.75em;

        i {
            top:    1px;
        }
    }
}



//~~ Variables, aka the Control Panel

$inner-width:   800px;



//~~ "Constants"

$bg-tex-blue:   $swatch-blue url(/static/img/newhome/bg/blue.jpg) top center;
$bg-tex-green:  $swatch-green url(/static/img/newhome/bg/green.jpg) top center;


.displaced { @include displace; }



body.admin {
    padding-top: 40px;
}


//~~ Page Title

#title {
    &.blue { background: $bg-tex-blue; }
    &.green { background: $bg-tex-green; }
}

.inner {
    width:      $inner-width;
    margin:     0 auto;
    position:   relative;
}



//~~ Tab Controls (Setup + Installation + Entries)

#rafl-controls {
    $_ctrl-height:  72px;
    $_border-color-oldbrowser: mix(#fff, $swatch-blue, 50%);
    $_border-color: rgba(#fff, 0.5);

    border-top: 1px solid $_border-color-oldbrowser;
    border-top: 1px solid $_border-color;

    &, ul {
        height: $_ctrl-height;
        position:   relative;
    }

    .inner {
        border-left:    1px solid $_border-color-oldbrowser;
        border-right:   1px solid $_border-color-oldbrowser;
        border-left:    1px solid $_border-color;
        border-right:   1px solid $_border-color;
    }

    li {
        display:    block;
        float:      left;

        &.pull-right {
            float:  right;
        }
    }

    a {
        @include inline-block;
        @include weight-semibold;
        @include font-ease;

        text-decoration:    none;
        color:              #fff;
        font-size:          em(20);

        margin-left:        -1px;

        position:           relative;

        line-height:        $_ctrl-height;
        height:             $_ctrl-height;

        padding:            0 24px 0 62px;

        border-right:       1px solid rgba(#fff, 0.3);

        i {
            $_height:   30px;

            width:              32px;
            height:             $_height;

            position:           absolute;
            top:                50%;
            margin-top:         - ($_height / 2);
            margin-left:        -42px;

            display:            block;
            background-image:   url(/static/img/icons/tab-sprite.png);
        }

        &#ctrl-entries {
            border-right:   none;

            i {
                margin-right:   12px;
            }
        }

        &#ctrl-admin {
            width:      32px;
            padding:    0 20px;
            
            i {
                margin-left:    0;
            }
        }

        &:hover {
            background:     mix(#fff, $swatch-blue, 20%);
            background:     rgba(#fff, 0.2);
        }

        &:active {
            @include translateY(2px);
        }

        &.active {
            top:            -10px;
            padding-bottom: 5px;
            padding-top:    5px;

            color:          $swatch-blue;
            background:     #fff;
        }
    }
}

#ctrl-admin {
    span {
        @include displace;
    }
}

#ctrl-setup i           { }
#ctrl-installation i    { @include sprite-rect(32px,0); }
#ctrl-entries i         { @include sprite-rect(64px,0); }
#ctrl-admin i           { @include sprite-rect(96px,0); }

#ctrl-setup.active i        { @include sprite-rect(0,30px); }
#ctrl-installation.active i { @include sprite-rect(32px,30px); }
#ctrl-entries.active i      { @include sprite-rect(64px,30px); }




//~~ Forms

input[type="text"], input[type="password"], textarea {
    @include border-radius(3px);
    border: 1px solid $swatch-faintgrey;

    color:  $swatch-lgrey;

    #main-col &, .raflModal & {
        @extend .large-input;
        @include border-radius(3px);

        font-size:  em(18);
    }

    #main-col .form-item & {
	// See .large-input for where these values come from
	width: $main-col-width - 2px /*border*/ - 32px /*padding*/;
    }

    .raflModal & {
        // TODO: this is not DRY
        width:      $main-col-width - 120px - 32px - 2px;
    }

    #sidebar & {
        width:      $sidebar-width - $sidebar-padding - 12px - 2px;
        padding:    em(8, 14) em(5, 14);
        font-size:  em(14);
    }
}

input[type="checkbox"] {
    float:          left;
    margin-top:     1px;
    margin-right:   0.75em;

    &:checked ~ span {
        @include weight-semibold;
    }
}

label.checkbox {
    &:hover {
        text-decoration:    underline;
        cursor:             pointer;
    }
}

#sidebar textarea {
    padding:    em(8, 8) em(5.5, 8);
}

.success {
    @include sprite-rect(0, 66px);
    background-image:   url(/static/img/icons/edit-page-sprite.png);
    width:              27px;
    height:             20px;
}

.err-label {
    color:          $swatch-orange;
    position:       relative;

    padding-left:   1em;

    &:before {
        content:    "√ó ";
        font-size:  1.5em;
        position:   absolute;
        top:        50%;
        margin-top: -0.45em;
        left:       0;
    }
}

label {
    #main-col .form-item > & {
	@extend .main-col-h2;
	display: block;
    }
}

.submit-box {
    @include micro-clearfix;
    
    a.submit {
	@include button("blue", "right");
	float: right;
    }
}



//~~ Page-specific Content

#page-content {
    /*overflow:   hidden;*/
    @include micro-clearfix;
    margin-top: em(60);
}


//~~ Main Column

.main-col-h2 {
    @include font-ease;
    @include weight-semibold;

    font-size:      em(32);
    color:          $swatch-lgrey;

    margin-bottom:  em(30, 32);
}

#main-col {
    float:      left;
    width:      $main-col-width;

    color:      $swatch-lgrey;

    h2 {
	@extend .main-col-h2;
    }   

    section {
        margin-bottom:  em(60);
    }
}



//~~ Sidebar

#sidebar {
    width:          $sidebar-width - $sidebar-padding;
    float:          right;

    padding-left:   $sidebar-padding;

    h2 {
        @include weight-semibold;
        @include font-ease;

        color:          $swatch-lgrey;

        font-size:      em(16);
        margin-bottom:  em(10);

        position:       relative;
    }

    small {
        font-size:      em(10);
        letter-spacing: 1px;
        color:          mix($swatch-lgrey, #fff, 70%);
        font-style:     italic;
        line-height:    1.1em;
        display:        block;
        margin-top:     -0.5em;
        margin-bottom:  0.5em;
    }

    section {
        margin-bottom:  em(36);

        &.ping {
            @include transform(scale(1.025));
            @include transition(0.1s);

            $_w: 0;
            $_c: $swatch-green;
            $_b: 3px;
            @include box-shadow($_w     0       $_b     $_c,
                                -$_w    0       $_b     $_c,
                                0      (-$_w)   $_b     $_c,
                                0       $_w     $_b     $_c);
        }

        &.unping {
            @include transform(scale(1));
            @include transition(0.1s);
            @include box-shadow(0);
        }
    }
}

#inline-faq {
    @include border-radius(5px);
    background: mix($swatch-faintgrey, #fff, 10%);
    padding:    1em;

    h2 {
        @include weight-semibold;
        margin-top:     0.5em;
        margin-bottom:  1em;
        font-size:      1.25em;
    }

    li {
        margin-bottom:      1em;
        padding-right:      0.5em;
    }

    a {
        color:              $swatch-blue;
        text-decoration:    none;
        line-height:        1.4em;

        &:after {
            content:    ' ¬ª';
            color:      $swatch-faintgrey;
        }

        &:hover {
            text-decoration:    underline;

            &:after {
                color:  $swatch-blue;
            }
        }
    }
}



//~~ Modals

.raflModal {
    $w: 2px;

    padding:    44px 60px;
    width:      $main-col-width - 120px;

    display:    block;
    position:   absolute;

    border:     10px solid $swatch-lgrey;

    background: #fff;

    z-index:    1000;

    &:after {
	content: '';
	position: absolute;
	top: 100%;
	border-bottom: 10px solid red; // $swatch-lgrey;
	border-top: 10px solid white;
	border-left: 2px solid green;
	margin-left: 242px;
	border-radius: 10px;
	left: 0;

	height: 493px;
	width: 493px;
	@include transform(scaleY(0.5));
    }

    h2 {
        @include weight-semibold;
        @include font-ease;

        font-size:      em(32);
        color:          $swatch-lgrey;
        margin-bottom:  em(16, 32);
    }

    p {
        color:  $swatch-lgrey;
    }

    small {
        display:        block;
        padding-top:    0.75em;
        color:          mix($swatch-lgrey, #fff, 75%);
        font-family:    Helvetica, sans-serif;
        font-size:      0.75em;
        font-style:     italic;
        line-height:    1.6em;
    }

    fieldset {
        border-top:     1px solid $swatch-faintgrey;

        margin-top:     1em;
        padding-top:    1em;
        
        &.first {
            border-top:     none;
            margin-top:     0;
            padding-top:    0;
        }
    }

    .submit-box {
        margin-top:     1em;
        padding-top:    44px;
        border-top:     1px solid $swatch-faintgrey;

        a.submit {
            float:  right;
        }

        .closelink {
            @include weight-semibold;
            line-height:    em(53, 12);
            font-size:      em(12);
            text-transform: uppercase;
            color:          $swatch-lgrey;
        }
    }

    &.shutting, &.shut {
        @include transition(0.15s);
        @include opacity(0.5);
        /*height:     0;*/
        overflow:   hidden;

        h2 {
            @include transition(0.25s);
            color:  #fff;
        }
    }

    &.pre-discarding {
        @include transition(all 100ms cubic-bezier(0.390, 0.575, 0.565, 1.000));
        @include translateX(150px);
        @include opacity(0.8);
        height: 0;
        width:  160px;
    }

    &.discarding {
        @include opacity(0);
        @include transition(all 170ms ease-in-out);
        @include translateX(-1000px);
        height:     0;
        width:      0;
        padding:    0;
    }

    label {
        display:        block;
        color:          $swatch-lgrey;
        padding-top:    1em;
        padding-bottom: 0.5em;
    }
}

#raflModal-overlay {
    display:    none;

    position:   fixed;
    width:      100%;
    height:     100%;
    background: rgba(#fff, 0.8);

    top:        0;
    right:      100%;

    border-top: 10px solid $swatch-lgrey;

    &.shown {
        @include transition(0.15s);
        @include opacity(1);
        right:  0;
    }

    &.hiding {
        @include transition(0.15s);
        right:  100%;
    }

    &.discarding {
        @include transition(0.25s);
        right:  100%;
        top:    0;
        left:   auto;
    }
}



//~~ Admin Bar

#adminbar {
    $_bg: $swatch-lgrey;

    opacity: 0.1;

    position:   absolute;
    top:        0;
    width:      100%;
    text-align: center;

    background: $_bg;

    &:hover {
	opacity: 1;
	@include transition(all 0.1s ease-in-out);
	@include box-shadow(0 12px 24px rgba(#000, 0.2));
    }

    li {
        @include inline-block;
        margin:     0;
        padding:    0;

        &:last-child {
            border-right:   1px solid darken($_bg, 10%);
        }
    }

    a {
        @include inline-block;
        padding:            0.75em 1em;
        color:              #fff;
        border-left:        1px solid darken($_bg, 10%);
        text-decoration:    none;

        &:hover {
            @include box-shadow(inset 0 1px 0 brighten($_bg, 100%));
            @include text-shadow(1px 1px 0 rgba(#000, 0.2));
            background:     rgba(#fff, 0.09);
        }
    }
}



//~~ Tooltips

.tipsy {
    @include border-radius(3px);

    position:   absolute;
    background: #333;
    color:      #fff;

    padding:    0.25em 0.325em;

    .tipsy-arrow-n { @include triangle(4px, #333, "up") }
    .tipsy-arrow-s { @include triangle(4px, #333, "down") }
    .tipsy-arrow-n { @include triangle(4px, #333, "up") }
    .tipsy-arrow-w { @include triangle(4px, #333, "up") }
}



